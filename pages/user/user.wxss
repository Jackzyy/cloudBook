
.personcenter{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background: #FDEFC8;
  width: 330px;
  height: 110px;
  border-radius: 40px;
  margin: 20px 10px;
}
.header-top{
  display: flex;
}

.header-img image{
  margin-top: 30rpx;
  width: 150rpx;
  height: 150rpx;
  border-radius: 100%;
}
.header-r{
  margin-top: 30rpx;
  margin-left: 20rpx;
  height: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.header-r .title{
  font-weight: 700
}

.header-label{
  font-size: 16px;
}
.header-bottom{
  display: flex;
  justify-content: space-between;
  padding: 15rpx 150rpx;
  font-size: 16px;
}
.bottom-text{
  text-align: center;
}
.collect-book{
  background-color: #F8F4F1;
  width: 150px;
  height: 50px;
  border-radius: 20px;
  margin-top: 150px
}
.btn{
  width: 180px;
  height: 50px;
  border-radius: 20px;
  margin-top: 150px
}
